<body>
	<div class="output">
		<?php
			function fibonacci($n){
				$num1=0;
				$num2=1;
				echo $num1 ." ";
				echo $num2 ." ";

				for($i=0;$i<$n;$i++){
					$num3=$num1+$num2;
					echo $num3 ." ";
					$num1=$num2;
					$num2=$num3;
				}
			}
			fibonacci(20)
		?>

	</div>
